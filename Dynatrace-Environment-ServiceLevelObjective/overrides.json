{
  "CREATE": {
    "/DynatraceAccess": "<DYNATRACE_TOKEN>",
    "/DynatraceEndpoint": "<DYNATRACE_ENDPOINT>",
    "/MetricExpression": "(100)*(builtin:service.errors.server.successCount:splitBy())/(builtin:service.requestCount.server:splitBy())",
    "/Filter": "type(\"SERVICE\"),entityName(\"My service\")",
    "/Target": 75,
    "/Warning": 80,
    "/ErrorBudgetBurnRate": {
      "FastBurnThreshold": 10,
      "BurnRateVisualizationEnabled": true
    },
    "/Timeframe": "-1w"
  },
  "UPDATE": {
    "/MetricExpression": "(100)*(builtin:service.errors.server.successCount:splitBy())/(builtin:service.requestCount.server:splitBy())",
    "/Filter": "type(\"SERVICE\"),entityName(\"My service\")",
    "/Target": 99.98,
    "/Warning": 99.99,
    "/ErrorBudgetBurnRate": {
      "FastBurnThreshold": 10,
      "BurnRateVisualizationEnabled": true
    },
    "/Timeframe": "-1w"
  }
}